{
  "name": "recent PDFs",
  "description": "makes for easy access to recently viewed PDF files, great for students",
  "version": "1.0",
  "default_locale": "en",
  "icons": {"32":"icon.png"},
  "manifest_version": 2,
  "homepage_url": "https://github.com/alexweininger/recent-pdfs",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "pdficon.png"
  },
  "permissions": [
    "http://*.google.com/",
    "https://*.google.com/",
    "history",
    "tabs",
    "chrome://favicon/"
  ],
  "content_security_policy": "img-src chrome://favicon; script-src 'self'; object-src 'self'",
  "offline_enabled": true
}