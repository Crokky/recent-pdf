{
    "name": "Recent PDF",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Makes for easy access to recently viewed PDF files, great for students",
    "homepage_url": "https://github.com/alexweininger/recent-pdfs",
    "icons": {
      "48": "icons/pdficon32.png"
    },
    "default_locale": "en",
    "browser_action": {
      "default_icon": "icons/pdficon32.png",
      "default_title": "recent pdfs",
      "default_popup": "src/browser_action/index.html"
    },
    "permissions": [
      "chrome://favicon/",
      "clipboardWrite",
      "contextMenus",
      "history",
      "tabs"
    ],
    "content_security_policy": "img-src chrome://favicon; script-src 'self'; object-src 'self'",
    "offline_enabled": true
  }