{
  "name": "recent PDFs",
  "description": "makes for easy access to recently viewed PDF files, great for students",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*.google.com/",
    "https://*.google.com/",
    "history",
    "tabs",
    "chrome://favicon/"
  ],
  "content_security_policy": "img-src chrome://favicon; script-src 'self'; object-src 'self'"
}